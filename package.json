{
  "name": "dicom.js",
  "version": "0.2.4",
  "description": "A small, fast, javascript DICOM renderer",
  "author": "wearemothership",
  "email": "nick@wearemotehrship.com",
  "license": "MIT",
  "repository": "mothershipsoft/dicom.js",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern",
    "start": "microbundle-crl watch --no-compress --format modern",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "dicom-character-set": "git+https://github.com/nickhingston/dicom-character-set.git#v1.0.2_nodefix",
    "jpeg-lossless-decoder-js": "git+https://github.com/rii-mango/JPEGLosslessDecoderJS.git#v2.0.4",
    "pako": "^1.0.10",
    "raw.macro": "^0.4.2",
    "twgl.js": "git+https://github.com/mothershipsoft/twgl.js#v4.19.1-node-canvas-webgl",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "babel-eslint": "^10.0.3",
    "blob-util": "^2.0.2",
    "canvas": "^2.6.1",
    "cross-env": "^7.0.2",
    "dicom-test-files": "git+https://github.com/mothershipsoft/dicom-test-files.git",
    "eslint-config-airbnb": "^18.2.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-canvas-webgl": "git+https://github.com/mothershipsoft/node-canvas-webgl.git#0.2.6-twgl",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3"
  },
  "files": [
    "dist"
  ]
}
